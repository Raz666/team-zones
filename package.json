{
  "name": "team-zones-workspace",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "mobile:dev": "npm run --workspace apps/mobile start",
    "mobile:android": "npm run --workspace apps/mobile android",
    "mobile:ios": "npm run --workspace apps/mobile ios",
    "mobile:web": "npm run --workspace apps/mobile web",
    "mobile:test": "npm run --workspace apps/mobile test",
    "mobile:typecheck": "npm run --workspace apps/mobile typecheck",
    "mobile:generate:policy-css": "npm run --workspace apps/mobile generate:policy-css"
  }
}
